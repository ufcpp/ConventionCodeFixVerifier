<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>ConventionCodeFixVerifier</id>
    <version>3.1.0.0</version>
    <title>ConventionCodeFixVerifier</title>
    <authors>Nobuyuki Iwanaga</authors>
    <owners>Nobuyuki Iwanaga</owners>
      <licenseUrl>https://github.com/ufcpp/ConventionCodeFixVerifier/blob/master/LICENSE</licenseUrl>
      <projectUrl>https://github.com/ufcpp/ConventionCodeFixVerifier/</projectUrl>
      <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Extends the CodeFixVerifier class in the "Analyzer with Code Fix" project template to read test cases from .cs files by naming convention.</description>
    <releaseNotes>Support tests for multiple actions on the code-fix.</releaseNotes>
    <copyright>Nobuyuki Iwanaga</copyright>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
  </metadata>
  <files>
      <file src="tools\*" target="tools\" />
      <file src="content\Helpers\*" target="content\Helpers\" />
      <file src="content\Verifiers\*" target="content\Verifiers\" />
      <file src="content\DataSource\*" target="content\DataSource\" />
      <file src="content\DataSource\UnitTest\TestMethod2\Diagnostic/*" target="content\DataSource\UnitTest\TestMethod2\Diagnostic/" />
      <file src="content\DataSource\UnitTest\TestMethod2\Expected/*" target="content\DataSource\UnitTest\TestMethod2\Expected/" />
      <file src="content\DataSource\UnitTest\TestMethod2\Source/*" target="content\DataSource\UnitTest\TestMethod2\Source/" />
  </files>
</package>